{
	"name": "grab-tools",
	"version": "0.17.0",
	"private": true,
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"host": "vite --host",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix --ignore-path .gitignore",
		"format": "prettier --write src/",
		"tauri": "tauri",
		"proto": "npx pbts -i src/assets/proto/proto.proto -o src/generated/proto.ts && echo 'You will have to manually clean up the file as it may have errors'",
		"helpers": "node meta/scripts/generate-helpers.mjs && npx prettier --write src/generated/helpers.ts"
	},
	"dependencies": {
		"@codemirror/lang-javascript": "^6.2.4",
		"@codemirror/lang-json": "^6.0.2",
		"@codemirror/legacy-modes": "^6.5.2",
		"@replit/codemirror-vim": "^6.3.0",
		"@sentry/vue": "^10.27.0",
		"codemirror": "^6.0.2",
		"jsfft": "^0.0.4",
		"pako": "^2.1.0",
		"pinia": "^2.0.32",
		"pinia-plugin-persistedstate": "^3.1.0",
		"protobufjs": "^7.2.5",
		"three": "^0.157.0",
		"vue": "^3.4.30",
		"vue-router": "^4.1.6"
	},
	"devDependencies": {
		"@rushstack/eslint-patch": "^1.2.0",
