		"@vitejs/plugin-vue": "^4.0.0",
		"@vue/eslint-config-prettier": "^7.1.0",
		"eslint": "^8.34.0",
		"eslint-plugin-vue": "^9.9.0",
		"postcss-nesting": "^13.0.1",
		"prettier": "^2.8.4",
		"prettier-plugin-organize-imports": "^4.3.0",
		"terser": "^5.44.0",
		"typescript": "^5.9.3",
		"vite": "^4.1.4",
		"vite-plugin-mkcert": "^1.14.0",
		"vue-tsc": "^3.2.2"
	}
